{
  "name": "mojiito",
  "description": "Mojiito is a JavaScript framework, heavily inspired by Angular and Ember, which brings the benefits of components, services, ... to your (static) website.",
  "author": "Mojiito Team",
  "version": "2.0.0-alpha.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/mojiito/mojiito"
  },
  "bugs": "https://github.com/mojiito/mojiito/issues",
